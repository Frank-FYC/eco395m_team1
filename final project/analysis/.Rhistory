compscorefunc <- function(year,column1,column2,age1){
df_comscorefunc <- subset(df,calc_age(year,df$DOB_Yr_Child)==age1)
df_comscorefunc_result <- cbind(df_comscorefunc[,column1],df_comscorefunc[,column2])
}
